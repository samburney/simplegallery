{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"require": {
		"laravel/framework": "4.0.*",
		"anahkiasen/former": "dev-develop",
		"fideloper/proxy": "dev-master",
		"sifnt/sifnt-util": "dev-master",
		"barracudanetworks/archivestream-php": "0.2.5",
		"jasig/phpcas": "1.3.2"
	},
	"repositories": [
	    {
	        "type": "composer",
	        "url": "http://composer.sifnt.wan"
	    },
		{
			"type": "package",
			"package": {
				"name": "jasig/phpcas",
				"version": "1.3.2",
				"dist": {
					"url": "http://downloads.jasig.org/cas-clients/php/1.3.2/CAS-1.3.2.tgz",
					"type": "tar"
				},
				"autoload" : {
					"classmap": ["CAS-1.3.2/"]
				}
			}
		}
	],
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
